{
  "buildCommand": "./vercel-build.sh",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "src/app/api/process-video/route.ts": {
      "memory": 1024,
      "maxDuration": 60
    },
    "src/app/api/process-video-python/route.py": {
      "runtime": "python3.9",
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/process-video-python",
      "destination": "/api/process-video-python/route.py"
    }
  ]
} 